<p id="notice"><%= notice %></p>

<h1>Meals</h1>

<table>
  <thead>
    <tr>
      <th>Eaten at</th>
      <th>Meal</th>
      <th>Meal source</th>
      <th>I1</th>
      <th>I2</th>
      <th>I3</th>
      <th>I4</th>
      <th>I5</th>
      <th>I6</th>
      <th>I7</th>
      <th>I8</th>
      <th>I9</th>
      <th>I10</th>
      <th>I11</th>
      <th>I12</th>
      <th>I other</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meals.order(:eaten_at).each do |meal| %>
      <tr>
        <td><%= meal.eaten_at.strftime"%H" %>:<%= meal.eaten_at.strftime"%M" %> - <%= meal.eaten_at.strftime"%a" %> <%= meal.eaten_at.strftime"%d" %> <%= meal.eaten_at.strftime"%b" %></td>
        <td><%= meal.meal %></td>
        <td><%= meal.meal_source %></td>
        <td><%= meal.i1 %></td>
        <td><%= meal.i2 %></td>
        <td><%= meal.i3 %></td>
        <td><%= meal.i4 %></td>
        <td><%= meal.i5 %></td>
        <td><%= meal.i6 %></td>
        <td><%= meal.i7 %></td>
        <td><%= meal.i8 %></td>
        <td><%= meal.i9 %></td>
        <td><%= meal.i10 %></td>
        <td><%= meal.i11 %></td>
        <td><%= meal.i12 %></td>
        <td><%= meal.i_other %></td>
        <td><%= meal.notes %></td>
        <td><%= link_to 'Show', meal %></td>
        <td><%= link_to 'Edit', edit_meal_path(meal) %></td>
        <td><%= link_to 'Destroy', meal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meal', new_meal_path %>
